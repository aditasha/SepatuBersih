<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_nav_graph"
    app:startDestination="@id/orderDriverFragment">

    <fragment
        android:id="@+id/orderDriverFragment"
        android:name="com.aditasha.sepatubersih.presentation.driver.OrderDriverFragment"
        android:label="@string/order_list">
        <action
            android:id="@+id/action_orderDriverFragment_to_orderDetailDriverFragment"
            app:destination="@id/orderDetailDriverFragment" />
    </fragment>

    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.aditasha.sepatubersih.presentation.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/orderDetailDriverFragment"
        android:name="com.aditasha.sepatubersih.presentation.driver.OrderDetailDriverFragment"
        android:label="OrderDetailDriverFragment">
        <argument
            android:name="key"
            app:argType="string" />
    </fragment>

</navigation>